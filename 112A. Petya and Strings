#include <stdio.h>
#include <string.h>
int main ()
{
    char s1[100];
    char s2[100];
    gets(s1);
    gets(s2);

    int value = stricmp(s1,s2);

    if(value > 0)
    {
        printf("1\n");
    }

    else if(value < 0)
    {
        printf("-1\n");
    }

    else
    {
        printf("0\n");
    }

    return(0);
 }
