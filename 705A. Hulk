#include <stdio.h>
int main()
{
    int n, i;
    scanf("%d", &n);
    printf("I hate");
    i = 1;
    while (i < n)
    {
        if (i & 1)
        {
            printf(" that I love");
        }
        else
        {
            printf(" that I hate");
        }
        i++;
    }
    printf(" it");

    return 0;
}
