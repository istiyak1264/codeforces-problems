#include<stdio.h>
#include<math.h>
void testing(int a,int b,int c)
{
    if(a==b+c || b==c+a || c==a+b)
    {
        printf("YES \n");
    }
    else
    {
        printf("NO \n");
    }
}

int main()
{

    int a,b,c,t;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d %d %d",&a,&b,&c);
        //passing the values to the testing function.
        testing(a,b,c);
    }

    return 0;
}
