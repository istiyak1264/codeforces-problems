#include<stdio.h>
int main()
{
    int t;
    scanf("%d",&t);//t refers to number of test cases.
    while(t--)
    {
        int a,b;
        scanf("%d %d",&a,&b);
        int moves=0;

        //calculating total moves.
        if (a%b!=0)
        {
            moves=b-(a%b);
        }

        //printing total moves.
        printf("%d \n",moves);
    }
    return 0;
}
