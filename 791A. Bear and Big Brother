#include<stdio.h>
#include<math.h>
int main()
{
    int a,b,i;// a= Limark's weight, b=Bob's weight
    int count=0;

    //taking input from the user.
    scanf("%d %d",&a,&b);
    while(a>0 && b>0)
    {
        a=a*3;
        b=b*2;
        count++;
        if(a>b)
        {
            printf("%d",count);
            break;
        }
    }
    return 0;
}
